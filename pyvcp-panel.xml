<?xml version='1.0' encoding='UTF-8'?>
<pyvcp>
	<vbox>
		<relief>RIDGE</relief>
		<bd>6</bd>
		<hbox>
			<vbox>
				<label>
					<text>"Actual Spindle Speed:"</text>
					<font>("Helvetica",10)</font>
				</label>
				<bar>
					<halpin>"spindle-rpm"</halpin>
					<max_>9000</max_>
				</bar>
			</vbox>
			<vbox>
				<label>
					<text>"Set Spindle Speed:"</text>
					<font>("Helvetica",10)</font>
				</label>
				<bar>
					<halpin>"set-spindle-speed"</halpin>
					<max_>9000</max_>
				</bar>
			</vbox>
			<vbox>
				<led>
					<halpin>"spindle-at-speed-led"</halpin> 
					<size>30</size> 
					<on_color>"green"</on_color>
					<off_color>"red"</off_color>
				</led>
			</vbox>
		</hbox>
		<hbox>
			<vbox>
				<label>
					<text>"Feed Rate (IPM)"</text>
					<font>("Helvetica",10)</font>
				</label>
				<bar>
					<halpin>"feed-rate"</halpin>
					<max_>800</max_>
				</bar>
			</vbox>
			<vbox>
				<label>
					<text>"Spindle Torque"</text>
					<font>("Helvetica",10)</font>
				</label>
				<scale>
					<font>("Helvetica",10)</font>
					<width>"15"</width>
					<halpin>"spindle-torque-out"</halpin>
					<resolution>0.1</resolution>
					<orient>HORIZONTAL</orient>
					<initval>90</initval>
					<min_>0</min_>
					<max_>100</max_>
				</scale>
			</vbox>
		</hbox>
			<vbox>
			<vbox>
				<hbox>
					<vbox>
						<label>
							<text>"spindle Trq"</text>
							<font>("Helvetica",10)</font>
						</label>
						<bar>
							<halpin>"spindle-torque"</halpin>
							<min_>0</min_>
							<max_>100.0</max_>
							<bgcolor>"grey"</bgcolor>
							<fillcolor>"cyan"</fillcolor>
						</bar>
					</vbox>
					<vbox>
						<label>
							<text>"X Trq"</text>
							<font>("Helvetica",10)</font>
						</label>
						<bar>
							<halpin>"x-torque"</halpin>
							<min_>0</min_>
							<max_>100.0</max_>
							<bgcolor>"grey"</bgcolor>
							<fillcolor>"cyan"</fillcolor>
						</bar>
					</vbox>
				</hbox>
        
				<hbox>
					<vbox>
						<label>
							<text>"Y Trq"</text>
							<font>("Helvetica",10)</font>
						</label>
						<bar>
							<halpin>"y-torque"</halpin>
							<min_>0</min_>
							<max_>100.0</max_>
							<bgcolor>"grey"</bgcolor>
							<fillcolor>"cyan"</fillcolor>
						</bar>
					</vbox>
					<vbox>
						<label>
							<text>"Z Trq"</text>
							<font>("Helvetica",10)</font>
						</label>
						<bar>
							<halpin>"z-torque"</halpin>
							<min_>0</min_>
							<max_>100.0</max_>
							<bgcolor>"grey"</bgcolor>
							<fillcolor>"cyan"</fillcolor>
						</bar>
					</vbox>
				</hbox>
        
				<hbox>
					<vbox>
						<label>
							<text>"A Trq"</text>
							<font>("Helvetica",10)</font>
						</label>
						<bar>
							<halpin>"a-torque"</halpin>
							<min_>0</min_>
							<max_>100.0</max_>
							<bgcolor>"grey"</bgcolor>
							<fillcolor>"cyan"</fillcolor>
						</bar>
					</vbox>
					<vbox>
						<label>
							<text>"B Trq"</text>
							<font>("Helvetica",10)</font>
						</label>
						<bar>
							<halpin>"b-torque"</halpin>
							<min_>0</min_>
							<max_>100.0</max_>
							<bgcolor>"grey"</bgcolor>
							<fillcolor>"cyan"</fillcolor>
						</bar>
					</vbox>
				</hbox>

				<hbox>
					<vbox>
						<label>
							<text>"Board Temp"</text>
							<font>("Helvetica",10)</font>
						</label>
						<bar>
							<halpin>"temp1"</halpin>
							<min_>15</min_>
							<max_>70.0</max_>
							<bgcolor>"grey"</bgcolor>
							<fillcolor>"red"</fillcolor>
						</bar>
					</vbox>
					<vbox>
						<label>
							<text>"Spindle M. Temp"</text>
							<font>("Helvetica",10)</font>
						</label>
						<bar>
							<halpin>"temp2"</halpin>
							<min_>15</min_>
							<max_>70.0</max_>
							<bgcolor>"grey"</bgcolor>
							<fillcolor>"red"</fillcolor>
						</bar>
					</vbox>
				</hbox>
				<hbox>
					<vbox>
						<label>
							<text>"Spdl Top Temp"</text>
							<font>("Helvetica",10)</font>
						</label>
						<bar>
							<halpin>"temp3"</halpin>
							<min_>15</min_>
							<max_>70.0</max_>
							<bgcolor>"grey"</bgcolor>
							<fillcolor>"red"</fillcolor>
						</bar>
					</vbox>
					<vbox>
						<label>
							<text>"Spndl Bott Temp"</text>
							<font>("Helvetica",10)</font>
						</label>
					<bar>
						<halpin>"temp4"</halpin>
						<min_>15</min_>
						<max_>70.0</max_>
						<bgcolor>"grey"</bgcolor>
						<fillcolor>"red"</fillcolor>
					</bar>
					</vbox>
				</hbox>
		</vbox>
			<relief>RIDGE</relief>
				<button>
					<halpin>"reset-amplifiers"</halpin>
					<text>"Reset Amps"</text> 
				</button>
		<button>
			<halpin>"probe-start"</halpin>
			<text>"Probe Start"</text> 
		</button>
	</vbox>
	<vbox>
		<hbox>
			<label>
				<text>"Probe Tool Setter:"</text>
				<font>("Helvetica",8)</font>
			</label>
			<led>
				<halpin>"probe-inspection-select"</halpin> 
				<size>8</size> 
				<on_color>"blue"</on_color>
				<off_color>"green"</off_color>
			</led>
			<label>
				<text>"Status:"</text>
				<font>("Helvetica",8)</font>
			</label>
			<led>
				<halpin>"probe-in"</halpin> 
				<size>10</size> 
				<on_color>"red"</on_color>
				<off_color>"green"</off_color>
			</led>
		</hbox>
		<hbox>
			<label>
				<text>"Probe Error:"</text>
				<font>("Helvetica",8)</font>
			</label>
			<led>
				<halpin>"probe-error"</halpin> 
				<size>10</size> 
				<on_color>"green"</on_color>
				<off_color>"red"</off_color>
			</led>
			<label>
				<text>"Battery:"</text>
				<font>("Helvetica",8)</font>
			</label>
			<led>
				<halpin>"probe-battery"</halpin> 
				<size>10</size> 
				<on_color>"green"</on_color>
				<off_color>"red"</off_color>
			</led>
			</hbox>
		</vbox>
		<hbox>
			<label>
				<text>"Tool Number: "</text>
				<font>("Helvetica",16)</font>
				<justify>"center"</justify> 
			</label>
				<s32>
				<halpin>"tool-number"</halpin>
				<font>("Helvetica",16)</font>
				<format>"03d"</format>
				<justify>"center"</justify> 
			</s32>
			<led>
					<halpin>"py-tc-changed"</halpin> 
					<size>30</size> 
					<on_color>"green"</on_color>
					<off_color>"red"</off_color>
			</led>
			<led>
					<halpin>"py-tc-error"</halpin> 
					<size>30</size> 
					<on_color>"red"</on_color>
					<off_color>"green"</off_color>
			</led>
		</hbox>
	</vbox>
</pyvcp>
